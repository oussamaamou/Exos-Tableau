#include<stdio.h>

int main(){
    int i, j, n, Echg;
    printf("Veuillez entrer le nombre des elements:");
    scanf("%d",&n);
    int Entier[n];
    for(i=0;i<n;i++){
        printf("Veuillez entrer l'element num %d :", i+1);
        scanf("%d",&Entier[i]);
    }
    Echg = 0;
    for(i=0;i<n;i++){
        for(j=1;j<n;j++){
            if (Entier[i]<Entier[j])
                 Echg = Entier[j];
                 Entier[j] = Entier[i];
                 Entier[i] = Echg;

        }
    }
    printf("\tLes elements du tableau en ordre croissant:\n");
    for(i=0;i<n;i++)
           printf("%d \n",Entier[i]);

    return 0;
}
